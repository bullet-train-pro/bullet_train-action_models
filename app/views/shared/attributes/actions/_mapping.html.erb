<% object ||= current_attributes_object %>
<% strategy ||= current_attributes_strategy || :none %>
<% url ||= nil %>

<% if object.send(attribute).present? %>
  <%= render 'shared/attributes/attribute', object: object, attribute: attribute, strategy: strategy, url: url do %>
    <%= render 'shared/well' do |p| %>
      <% p.content_for :raw_body do %>
        <table class="table">
          <thead>
            <th style="width: 49%;">CSV Column</th>
            <th style="width: 51%;">Project Field</th>
          </thead>
          <tbody>
            <% object.send(attribute).each do |key, value| %>
              <tr>
                <td><code><%= key %></code></td>
                <td><strong><%= value.present? ? t("#{object.subject.name.underscore.pluralize}.fields.#{value}.heading") : "Don't Import" %></strong></td>
              </tr>
            <% end %>
          </tbody>
        </table>
      <% end %>
    <% end %>
  <% end %>
<% end %>
